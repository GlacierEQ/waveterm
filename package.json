{
  "name": "waveterm",
  "version": "0.12.1",
  "description": "Wave Terminal - AI-Enhanced Terminal with Multi-Agent System",
  "main": "emain/main.js",
  "homepage": "https://www.waveterm.dev",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wavetermdev/waveterm.git"
  },
  "scripts": {
    "dev": "concurrently \"npm run build:frontend:dev\" \"electron emain/main.js\"",
    "start": "electron emain/main.js",
    "build:dev": "vite build --mode development",
    "build:prod": "vite build --mode production",
    "build:frontend:dev": "vite build --mode development --watch",
    "electron:package": "electron-builder",
    "electron:pack": "electron-builder --dir",
    "electron:dist": "electron-builder --publish=never",
    "validate": "npm run typecheck && npm run lint && npm run test",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "test": "vitest run",
    "test:watch": "vitest",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "k8s:setup": "node scripts/setup-kubernetes.js",
    "k8s:check": "node scripts/check-kubernetes.js",
    "k8s:install-tools": "node scripts/install-k8s-tools.js",
    "mcp:k8s": "concurrently \"node mcp-servers/kubectl-mcp-server.js\" \"node mcp-servers/helm-mcp-server.js\" \"node mcp-servers/minikube-mcp-server.js\"",
    "ai:k8s": "concurrently \"npm run dev\" \"npm run mcp:k8s\""
  },
  "keywords": [
    "terminal",
    "ai",
    "multi-agent",
    "mcp",
    "kubernetes",
    "kubectl",
    "helm",
    "minikube",
    "docker",
    "container",
    "devops",
    "electron",
    "typescript",
    "react"
  ],
  "author": "Command Line Inc",
  "license": "Apache-2.0",
  "dependencies": {
    "@ai-sdk/react": "^2.0.76",
    "@floating-ui/react": "^0.27.16",
    "@monaco-editor/loader": "^1.5.0",
    "@monaco-editor/react": "^4.7.0",
    "@observablehq/plot": "^0.6.17",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@react-hook/resize-observer": "^2.0.2",
    "@table-nav/core": "^0.0.7",
    "@table-nav/react": "^0.0.7",
    "@tanstack/react-table": "^8.21.3",
    "@xterm/addon-fit": "^0.10.0",
    "@xterm/addon-search": "^0.15.0",
    "@xterm/addon-serialize": "^0.13.0",
    "@xterm/addon-web-links": "^0.11.0",
    "@xterm/addon-webgl": "^0.18.0",
    "@xterm/xterm": "^5.5.0",
    "ai": "^5.0.44",
    "base64-js": "^1.5.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "color": "^4.2.3",
    "colord": "^2.9.3",
    "css-tree": "^3.1.0",
    "dayjs": "^1.11.15",
    "debug": "^4.4.3",
    "electron-updater": "^6.6",
    "env-paths": "^3.0.0",
    "fast-average-color": "^9.5.0",
    "htl": "^0.3.1",
    "html-to-image": "^1.11.13",
    "immer": "^10.1.1",
    "jotai": "2.9.3",
    "mermaid": "^11.12.0",
    "monaco-editor": "^0.52.0",
    "monaco-yaml": "^5.4.0",
    "overlayscrollbars": "^2.12.0",
    "overlayscrollbars-react": "^0.5.6",
    "papaparse": "^5.5.3",
    "parse-srcset": "^1.0.2",
    "pngjs": "^7.0.0",
    "prop-types": "^15.8.1",
    "react": "^19.2.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^19.2.0",
    "react-frame-component": "^5.2.7",
    "react-hook-form": "^7.63.0",
    "react-markdown": "^9.0.3",
    "react-resizable-panels": "^3.0.6",
    "react-zoom-pan-pinch": "^3.7.0",
    "recharts": "^2.15.4",
    "rehype-highlight": "^7.0.2",
    "rehype-raw": "^7.0.0",
    "rehype-sanitize": "^6.0.0",
    "rehype-slug": "^6.0.0",
    "remark-flexible-toc": "^1.2.3",
    "remark-gfm": "^4.0.1",
    "remark-github-blockquote-alert": "^1.3.1",
    "rxjs": "^7.8.2",
    "semver": "^7.7.3",
    "shell-quote": "^1.8.3",
    "shiki": "^3.13.0",
    "sprintf-js": "^1.1.3",
    "streamdown": "^1.3.0",
    "tailwind-merge": "^3.3.1",
    "throttle-debounce": "^5.0.2",
    "tinycolor2": "^1.6.0",
    "unist-util-visit": "^5.0.0",
    "use-device-pixel-ratio": "^1.1.2",
    "winston": "^3.18.3",
    "ws": "^8.18.3",
    "yaml": "^2.7.1"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^3.2.7",
    "@eslint/js": "^8.57.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/addon-interactions": "^8.6.14",
    "@storybook/addon-links": "^8.6.14",
    "@storybook/blocks": "^8.6.14",
    "@storybook/builder-vite": "^8.6.14",
    "@storybook/react": "^8.6.14",
    "@storybook/react-vite": "^8.6.14",
    "@storybook/testing-library": "^0.2.2",
    "@tailwindcss/vite": "^4.1.16",
    "@types/lodash": "^4.17.0",
    "@types/node": "^20.14.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vitejs/plugin-react": "^4.3.1",
    "concurrently": "^8.2.2",
    "electron": "^31.3.1",
    "electron-builder": "^24.13.3",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "prettier": "^3.3.3",
    "typescript": "^5.5.4",
    "vite": "^5.4.2",
    "vitest": "^2.0.5"
  },
  "build": {
    "appId": "dev.waveterm.app",
    "productName": "Wave Terminal",
    "directories": {
      "output": "make"
    },
    "files": [
      "dist/**/*",
      "emain/**/*",
      "node_modules/**/*",
      "package.json"
    ],
    "mac": {
      "category": "public.app-category.developer",
      "target": [
        {
          "target": "dmg",
          "arch": [
            "x64",
            "arm64"
          ]
        }
      ]
    },
    "linux": {
      "target": [
        {
          "target": "AppImage",
          "arch": [
            "x64",
            "arm64"
          ]
        },
        {
          "target": "deb",
          "arch": [
            "x64",
            "arm64"
          ]
        }
      ]
    },
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "arm64"
          ]
        }
      ]
    }
  }
}
